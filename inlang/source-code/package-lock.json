{
  "name": "inlang-source-code",
  "lockfileVersion": 3,
  "requires": true,
  "packages": {
    "": {
      "name": "inlang-source-code",
      "workspaces": [
        "../../lix/source-code/*",
        "./*"
      ]
    },
    "../../lix/source-code": {
      "name": "lix-source-code",
      "workspaces": [
        "./*"
      ]
    },
    "../../lix/source-code/client": {
      "name": "@lix-js/client",
      "dependencies": {
        "typescript": "^4.1.2"
      }
    },
    "../../lix/source-code/node_modules/typescript": {
      "version": "4.9.5",
      "license": "Apache-2.0",
      "bin": {
        "tsc": "bin/tsc",
        "tsserver": "bin/tsserver"
      },
      "engines": {
        "node": ">=4.2.0"
      }
    },
    "../../lix/source-code/sdk": {
      "name": "@lix-js/sdk",
      "dependencies": {
        "@lix-js/client": "*",
        "@lix-js/server": "*",
        "typescript": "^4.1.2"
      }
    },
    "../../lix/source-code/server": {
      "name": "@lix-js/server",
      "dependencies": {
        "typescript": "^4.1.2"
      }
    },
    "node_modules/@inlang/sdk": {
      "resolved": "sdk",
      "link": true
    },
    "node_modules/@lix-js/client": {
      "resolved": "../../lix/source-code/client",
      "link": true
    },
    "node_modules/@lix-js/sdk": {
      "resolved": "../../lix/source-code/sdk",
      "link": true
    },
    "node_modules/@lix-js/server": {
      "resolved": "../../lix/source-code/server",
      "link": true
    },
    "node_modules/typescript": {
      "version": "4.9.5",
      "license": "Apache-2.0",
      "bin": {
        "tsc": "bin/tsc",
        "tsserver": "bin/tsserver"
      },
      "engines": {
        "node": ">=4.2.0"
      }
    },
    "sdk": {
      "name": "@inlang/sdk",
      "dependencies": {
        "@lix-js/client": "*",
        "typescript": "^4.1.2"
      }
    }
  }
}
